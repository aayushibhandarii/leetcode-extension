{
    "manifest_version" : 3,
    "name" : "Leetcode Helper",
    "version" : "1.0.0",

    "permissions" : ["scripting","storage","activeTab","windows"],

    "action" : 
        {
            "default_icon" : "logo.png"
        }
    ,
    "background" :{
        "service_worker" : "background.js"
    },
    "options_page" : "options.html",
    "host_permissions" : ["<all_urls>"]
}